[
  {
    "description": "这是一个带header的post请求",
    "request": {
      "uri": "/post/with/header",
      "method": "post",
      "headers": {
        "content-type": "application/json"
      },
      "json": {
        "name": "wanglaosi",
        "sex": "woman"
      }
    },
    "response": {
      "status": 200,
      "json": {
        "wanglaoshi": "success",
        "status": 1
      },
      "headers": {
        "Content-Type": "text/html;charset=gbk"
      }
    }
  }
]