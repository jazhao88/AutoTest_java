<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.code.model">
    <!-- 获取登录接口case -->
    <select id="loginCase" parameterType="Integer" resultType="com.code.LoginCase">
        select * from loginCase where id=#{id}
    </select>
    <!-- 添加用户接口case -->
    <select id="addUserCase" parameterType="Integer" resultType="com.code.AddUserCase">
        select * from addUserCase where id = #{id}
    </select>
    <!-- 获取用户信息case -->
    <select id="getUserInfoCase" parameterType="Integer" resultType="com.code.GetUserInfoCase">
        select * from getUserInfoCase where id = #{id}
    </select>
    <!-- 获取用户列表case -->
    <select id="getUserListCase" parameterType="Integer" resultType="com.code.GetUserListCase">
        select * from getUserListCase where id = #{id}
    </select>
    <!-- 更新/删除用户case -->
    <select id="updateUserInfoCase" parameterType="Integer" resultType="com.code.UpdateUserInfoCase">
        select * from updateUserCase where id = #{id}
    </select>
</mapper>